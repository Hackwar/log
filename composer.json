{
    "name": "joomla/log",
    "type": "joomla-package",
    "description": "Joomla Log Package",
    "keywords": ["joomla", "framework", "log"],
    "homepage": "https://github.com/joomla-framework/log",
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.3.10",
        "joomla/date": "~1.0",
        "joomla/filesystem": "~1.0"
    },
    "require-dev": {
        "joomla/database": "~1.0",
        "joomla/test": "~1.0",
        "phpunit/phpunit": "4.*",
	    "phpunit/dbunit": "~1.2",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\Log\\": "src/",
            "Joomla\\Log\\Tests\\": "Tests/"
        }
    }
}
